    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint:css": "stylelint './src/**/*.jsx'",
    "lint": "eslint './src/**/*.jsx' --fix"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.3.31",
    "gatsby-plugin-react-helmet": "^3.0.12",
